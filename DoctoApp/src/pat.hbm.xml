<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
<class name="dao.patient" table="patient">

<id name="pid">
<generator class="increment"></generator>
</id>
<property name="ppass" />
<property name="pname" />
<property name="mob" />
<property name="age" />
<property name="sex" />
<property name="addr" />

</class>
</hibernate-mapping>